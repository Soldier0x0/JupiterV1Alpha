# Jupiter SIEM - Development Environment Configuration
# RENAME THIS FILE TO .env.development AFTER CREATION
# DO NOT COMMIT THIS FILE TO VERSION CONTROL

# =============================================================================
# DEVELOPMENT OVERRIDES
# =============================================================================
APP_ENVIRONMENT=development
DEBUG=true
RELOAD=true
LOG_LEVEL=DEBUG

# =============================================================================
# API CONFIGURATION (Development)
# =============================================================================
API_URL=http://localhost:8001
WS_URL=ws://localhost:8001
REACT_APP_API_URL=http://localhost:8001
REACT_APP_WS_URL=ws://localhost:8001
REACT_APP_DEBUG=true

# =============================================================================
# DATABASE CONFIGURATION (Development)
# =============================================================================
MONGO_URL=mongodb://localhost:27017/jupiter_siem_dev
REDIS_URL=redis://localhost:6379

# =============================================================================
# SECURITY CONFIGURATION (Development)
# =============================================================================
JWT_SECRET_KEY=jupiter_siem_development_secret_key_not_for_production
ENABLE_RATE_LIMITING=false
ENABLE_2FA=false
SESSION_TIMEOUT=7200

# =============================================================================
# ADMIN CONFIGURATION (Development)
# =============================================================================
SUPER_ADMIN_EMAIL=admin@localhost
SUPER_ADMIN_PASSWORD=admin123
SUPER_ADMIN_TENANT_NAME=DevTenant

# =============================================================================
# EMAIL CONFIGURATION (Development)
# =============================================================================
EMAIL_HOST=localhost
EMAIL_PORT=1025
EMAIL_USER=dev@localhost
EMAIL_PASSWORD=dev_password
EMAIL_USE_TLS=false

# =============================================================================
# AI/ML CONFIGURATION (Development)
# =============================================================================
AI_ENABLED=false
AI_DEVICE=cpu
AI_MAX_MEMORY_USAGE=0.5
AI_QUANTIZATION=false
AI_BATCH_SIZE=1
AI_MAX_CONCURRENT_REQUESTS=1

# =============================================================================
# MONITORING CONFIGURATION (Development)
# =============================================================================
ENABLE_METRICS=false
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=10

# =============================================================================
# CLOUDFLARE CONFIGURATION (Development)
# =============================================================================
CLOUDFLARE_ENABLED=false
REACT_APP_CLOUDFLARE_ENABLED=false

# =============================================================================
# SSL/TLS CONFIGURATION (Development)
# =============================================================================
SSL_ENABLED=false
SSL_REDIRECT_HTTP=false

# =============================================================================
# FIREWALL CONFIGURATION (Development)
# =============================================================================
UFW_ENABLED=false
FAIL2BAN_ENABLED=false

# =============================================================================
# THREAT INTELLIGENCE CONFIGURATION (Development)
# =============================================================================
THREAT_INTEL_ENABLED=false
THREAT_INTEL_API_KEY_VT=dev_key
THREAT_INTEL_API_KEY_ABUSE=dev_key
THREAT_INTEL_API_KEY_SHODAN=dev_key

# =============================================================================
# NOTIFICATION CONFIGURATION (Development)
# =============================================================================
NOTIFICATION_EMAIL_ENABLED=false
NOTIFICATION_SLACK_ENABLED=false
NOTIFICATION_WEBHOOK_ENABLED=false
REACT_APP_NOTIFICATION_EMAIL_ENABLED=false
REACT_APP_NOTIFICATION_SLACK_ENABLED=false
REACT_APP_NOTIFICATION_WEBHOOK_ENABLED=false

# =============================================================================
# CUSTOM CONFIGURATION (Development)
# =============================================================================
CUSTOM_FEATURES_ENABLED=true
EXPERIMENTAL_AI_FEATURES=true
BETA_FEATURES_ENABLED=true
REACT_APP_EXPERIMENTAL_FEATURES=true
REACT_APP_BETA_FEATURES=true

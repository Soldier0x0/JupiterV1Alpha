# Jupiter SIEM Frontend Environment Configuration
# RENAME THIS FILE TO .env AFTER CREATION
# DO NOT COMMIT THIS FILE TO VERSION CONTROL

# =============================================================================
# API CONFIGURATION
# =============================================================================
REACT_APP_API_URL=http://localhost:8001
REACT_APP_WS_URL=ws://localhost:8001
REACT_APP_ENVIRONMENT=production
REACT_APP_VERSION=2.0.0
REACT_APP_DEBUG=false

# =============================================================================
# FEATURE FLAGS
# =============================================================================
REACT_APP_AI_ENABLED=true
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_DEBUG_MODE=false
REACT_APP_EXPERIMENTAL_FEATURES=false
REACT_APP_BETA_FEATURES=false

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
REACT_APP_APP_NAME=Jupiter SIEM
REACT_APP_APP_TITLE=Jupiter SIEM - Security Information and Event Management
REACT_APP_APP_DESCRIPTION=Enterprise-grade SIEM solution for threat detection and response

# =============================================================================
# DOMAIN CONFIGURATION
# =============================================================================
REACT_APP_DOMAIN=siem.projectjupiter.in
REACT_APP_MAIN_DOMAIN=projectjupiter.in

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
REACT_APP_ENABLE_2FA=true
REACT_APP_SESSION_TIMEOUT=3600
REACT_APP_MAX_LOGIN_ATTEMPTS=5

# =============================================================================
# MONITORING CONFIGURATION
# =============================================================================
REACT_APP_ENABLE_METRICS=true
REACT_APP_METRICS_PORT=9090
REACT_APP_HEALTH_CHECK_INTERVAL=30

# =============================================================================
# CLOUDFLARE CONFIGURATION
# =============================================================================
REACT_APP_CLOUDFLARE_ENABLED=true
REACT_APP_CLOUDFLARE_EMAIL=harsha@projectjupiter.in

# =============================================================================
# NOTIFICATION CONFIGURATION
# =============================================================================
REACT_APP_NOTIFICATION_EMAIL_ENABLED=true
REACT_APP_NOTIFICATION_SLACK_ENABLED=false
REACT_APP_NOTIFICATION_WEBHOOK_ENABLED=false

# =============================================================================
# CUSTOM CONFIGURATION
# =============================================================================
REACT_APP_CUSTOM_FEATURES_ENABLED=true
REACT_APP_EXPERIMENTAL_AI_FEATURES=false
REACT_APP_BETA_FEATURES_ENABLED=false
